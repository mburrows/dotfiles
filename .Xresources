XTerm*faceName:         DejaVuSansMono Nerd Font Mono
XTerm*faceSize:         9
XTerm*saveLines:        9999
XTerm*scrollBar:        false
XTerm*scrollKey:        true
XTerm*scrollTtyOutput:  false
XTerm*visualBell:       true
XTerm*utf8:             1
XTerm*cjkWidth:         0

!Emacs.font:  Inconsolata:size=16
