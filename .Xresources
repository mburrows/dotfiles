URxvt*saveLines: 9999
URxvt.visualBell: true
!URxvt*background: #FBF8EF
URxvt.perl-ext-common: default,selection-to-clipboard,matcher
URxvt.matcher.button: 1
URxvt.url-launcher: /usr/bin/google-chrome
!URxvt.colorUL: #7780a1

rofi.terminal: urxvt
rofi.ssh-command: {terminal} -e {ssh-client} -X {host}

